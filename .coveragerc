[run]
source = src/
omit = 
    */tests/*
    */venv/*
    */__pycache__/*
    */migrations/*
    */node_modules/*
    .venv/*

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstractmethod
    class .*\bProtocol\):
    
precision = 2
show_missing = True
skip_covered = False

[html]
directory = htmlcov
title = Pokemon Card Scanner Coverage Report